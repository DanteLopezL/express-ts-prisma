// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name String 
  lastname String
  description String
  interests Interest[]
  profilePic String
  createdAt DateTime @default(now())
}

model Interest {
  id Int @id @default(autoincrement())
  name String
  users User[]
}